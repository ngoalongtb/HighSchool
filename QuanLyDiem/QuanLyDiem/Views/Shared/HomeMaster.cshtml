@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Document</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" type="text/css" href="~/public/client/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="~/public/client/bootstrap/css/bootstrap-theme.min.css">
    <script src="~/public/client/js/jquery.min.js"></script>
    <script src="~/public/client/bootstrap/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="~/public/client/css/style.css">
    <link rel="stylesheet" href="~/public/client/bootstrap/fonts/font-awesome.min.css">
</head>
<body>

    <nav class="navbar navbar-default" role="navigation">
        <div class="container-fuild">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                    <span class="sr-only"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>

            </div>

            <!-- Collect the nav links, forms, and other content for toggling -->

            <div class="collapse navbar-collapse navbar-ex1-collapse">
                <div class="container">
                    <div class="menu">
                        <ul class="nav navbar-nav">
                            <li class="active"><a href="#">Trang chủ</a></li>
                            <li><a href="#gioithieu">Giới thiệu</a></li>
                            <li><a href="#">Liên hệ</a></li>
                        </ul>
                    </div>
                </div>

                <!-- <form class="navbar-form navbar-left" role="search">
                    <div class="form-group">
                        <input type="text" class="form-control" placeholder="Nhập từ khóa">
                    </div>
                    <button type="submit" class="btn btn-default">Tìm kiếm</button>
                </form> -->

            </div>

        </div>
    </nav> <!-- end-nav -->

    <div class="container">
        <div class="row">
            <div class="col-md-8">
                <div id="myCarousel" class="carousel slide" data-ride="carousel">
                    <!-- Indicators -->
                    <ol class="carousel-indicators">
                        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                        <li data-target="#myCarousel" data-slide-to="1"></li>
                        <li data-target="#myCarousel" data-slide-to="2"></li>
                        <li data-target="#myCarousel" data-slide-to="2"></li>
                        <li data-target="#myCarousel" data-slide-to="2"></li>
                    </ol>

                    <!-- Wrapper for slides -->
                    <div class="carousel-inner">
                        <div class="item active">
                            <img src="~/public/client/images/slide1.jpg" alt="Los Angeles" style="width:100%;">
                        </div>
                        <div class="carousel-caption">

                        </div>


                        <div class="item">
                            <img src="~/public/client/images/slide2.jpg" alt="Chicago" style="width:100%;">
                        </div>

                        <div class="item">
                            <img src="~/public/client/images/van_nghe_1.jpg" alt="New york" style="width:100%;">
                        </div>
                        <div class="item">
                            <img src="~/public/client/images/giangduong.jpg" alt="New york" style="width:100%;">
                        </div>
                        <div class="item">
                            <img src="~/public/client/images/rachoi.jpg" alt="New york" style="width:100%;">
                        </div>
                    </div>

                    <!-- Left and right controls -->
                    <a class="left carousel-control" href="#myCarousel" data-slide="prev">
                        <span class="glyphicon glyphicon-chevron-left"></span>
                        <span class="sr-only">Previous</span>
                    </a>
                    <a class="right carousel-control" href="#myCarousel" data-slide="next">
                        <span class="glyphicon glyphicon-chevron-right"></span>
                        <span class="sr-only">Next</span>
                    </a>
                </div>

            </div>
            <div class="col-md-4" id="news">
                
            </div>

        </div>
    </div> <!-- end-slide -->


    <div class="container">
        <div class="row">
            <div class="col-md-8 ">
                @RenderBody()
            </div>


            <div class="col-md-4">
                <div class="col-md-12">
                    <h4 class="pm">Phần mềm tác nghiệp</h4>

                    <ul class="pm">
                        <li><a href=""><img src="~/public/client/images/phuhuynh.png" alt=""></a></li>
                        <li><a href=""><img src="~/public/client/images/tracuu.png" alt=""></a></li>
                        <li><a href=""><img src="~/public/client/images/hoidap.jpg" alt=""></a></li>
                        <li><a href=""><img src="~/public/client/images/tochuc.jpg" alt=""></a></li>
                    </ul>
                </div>


                <div class="col-md-12">
                    <h4 class="pm">Bản đồ vị trí</h4>

                    <img class="map" src="~/public/client/images/Map2.jpg" alt="">
                </div>

                <div class="col-md-12">
                    <h4 class="pm">Thư viện ảnh</h4>

                    <div id="myCarousel" class="carousel tv slide" data-ride="carousel">
                        <!-- Indicators -->
                        <ol class="carousel-indicators">
                            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                            <li data-target="#myCarousel" data-slide-to="1"></li>
                            <li data-target="#myCarousel" data-slide-to="2"></li>
                        </ol>

                        <!-- Wrapper for slides -->
                        <div class="carousel-inner">
                            <div class="item active">
                                <img src="~/public/client/images/slide1.jpg" alt="Los Angeles" style="width:100%;">
                            </div>
                            <div class="carousel-caption">

                            </div>


                            <div class="item">
                                <img src="~/public/client/images/slide2.jpg" alt="Chicago" style="width:100%;">
                            </div>

                            <div class="item">
                                <img src="~/public/client/images/van_nghe_1.jpg" alt="New york" style="width:100%;">
                            </div>
                        </div>

                        <!-- Left and right controls -->
                        <a class="left carousel-control" href="#myCarousel" data-slide="prev">
                            <span class="glyphicon glyphicon-chevron-left"></span>
                            <span class="sr-only">Previous</span>
                        </a>
                        <a class="right carousel-control" href="#myCarousel" data-slide="next">
                            <span class="glyphicon glyphicon-chevron-right"></span>
                            <span class="sr-only">Next</span>
                        </a>
                    </div>
                </div>


            </div>

        </div>
    </div>


    <nav class="navbar navbar-default" role="navigation">
        <div class="container-fuild">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                    <span class="sr-only"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>

            </div>

            <!-- Collect the nav links, forms, and other content for toggling -->

            <div class="collapse navbar-collapse navbar-ex1-collapse">
                <div class="container">
                    <div class="menu">
                        <ul class="nav navbar-nav">
                            <li class="active"><a href="#">Trang chủ</a></li>
                            <li><a href="#">Giới thiệu</a></li>
                            <li><a href="#">Liên hệ</a></li>
                        </ul>
                    </div>
                </div>

                <!-- <form class="navbar-form navbar-left" role="search">
                    <div class="form-group">
